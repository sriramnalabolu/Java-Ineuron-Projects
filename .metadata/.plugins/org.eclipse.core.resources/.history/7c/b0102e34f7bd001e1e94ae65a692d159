class MyThread extends Thread {
	@Override
	public void run() {
	    for (int i =1;i<=5; i++) {
	    	System.out.println("rama thread");
	        Thread.sleep(100);
	    }
	}	
}

public class Test{
    public static void main(String[] args){
        MyThread t = new MyThread();
        t.start();

        for(int i =1;i<=5;i++)
            System.out.println("sita thread");
    }
}
